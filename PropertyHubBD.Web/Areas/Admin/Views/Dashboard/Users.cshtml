@model List<PropertyHubBD.Web.Models.ApplicationUser>

@{
    ViewData["Title"] = "Manage Users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container mt-4">
    <h2>Manage Users</h2>
    <table class="table table-striped mt-3">
        <thead>
            <tr>
                <th>Username</th>
                <th>Email</th>
                <th>Full Name</th>
                <th>User Type</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.UserName</td>
                    <td>@item.Email</td>
                    <td>@item.FullName</td>
                    <td>@item.UserType</td>
                </tr>
            }
        </tbody>
    </table>
</div>
